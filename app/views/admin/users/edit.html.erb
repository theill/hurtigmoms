<h1>Editing user</h1>

<% form_for([:admin, @user]) do |f| %>
  <%= f.error_messages %>

	<%= f.text_field :email %>
	<%= f.text_field :company %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', admin_users_path %> | <%= link_to 'Destroy', [:admin, @user], :confirm => 'Are you sure?', :method => :delete %>
